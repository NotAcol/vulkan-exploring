-DASSERT=1
-DPROFILE=1
-DARENA_DEBUG=0
-DVULKAN_DEBUG=1
-DWAYLAND_DEBUG=1
-Wall
-Wno-excess-initializers
-Wno-missing-braces
-Wno-unused-function
-fuse-ld=lld
-O2
-mavx2
-mfma
-ffinite-math-only
-fno-signed-zeros
-freciprocal-math
-fassociative-math
-fno-exceptions 
-fno-trapping-math
-fno-math-errno
-pthread
-lvulkan
